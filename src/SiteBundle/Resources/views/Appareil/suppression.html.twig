{% extends 'base.html.twig' %}
{% block content %}
    <h2>Ajouter un appareil</h2>
    {% if supression == true %}
        <h3>Bravo !</h3>
        <p>L'appareil a bien été supprimé</p>
        {% else %}
            <h3>Erreur</h3>
            <p>L'appareil n'a pas été supprimé</p>
    {% endif %}
{% endblock %}